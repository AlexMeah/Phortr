{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "indent": [2, 4],
    "comma-dangle": [2, "never"],
    "new-cap": 0,
    "id-length": 0,
    "vars-on-top": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "padded-blocks": 0,
    "no-multiple-empty-lines": [2,{"max": 1}],
    "consistent-return": 0
  },
  "plugins": [
    "react"
  ]
}
